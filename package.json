{
  "name": "@nebulr-group/nblocks-ts-client",
  "version": "5.4.2",
  "exports": {
    "./engine": {
      "types": "./dist/engine/index.d.ts",
      "require": "./dist/engine/index.js",
      "default": "./dist/engine/index.js"
    },
    "./core-api": {
      "types": "./dist/core-api/index.d.ts",
      "require": "./dist/core-api/index.js",
      "default": "./dist/core-api/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "engine": ["./dist/engine/index.d.ts"],
      "core-api": ["./dist/core-api/index.d.ts"]
    }
  },  
  "description": "This library contains the low level NBlocks client that is shipped with all of our NBlocks plug-n-play experiences for different technologies.",  "files": [
    "/dist"
  ],
  "contributors": [
    "Oscar Söderlund <oscar@nebulr.group>",
    "Iman Pouya <iman@nebulr.group>"
  ],
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "prebuild": "rm -rf dist/*",
    "build": "tsc",
    "npm-publish": "npm run build && npm publish --access public",
    "npm-publish-beta": "npm run build && npm publish --access public --tag beta",
    "npm-publish-local": "npm run build && npm publish --registry  http://verdaccio:4873  --tag alpha",
    "test": "jest"
  },
  "author": "Oscar Söderlund & Iman Pouya",
  "keywords": [
    "api",
    "platform"
  ],
  "bugs": {
    "url": "https://github.com/orr3/nebulr-platform-lib/issues",
    "email": "support@nebulr.group"
  },
  "repository": {
    "repository": {
      "type": "git",
      "url": "https://github.com/orr3/nebulr-platform-lib"
    }
  },
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "axios-mock-adapter": "^1.21.2",
    "jest": "^27.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^5.6.3"
  },
  "dependencies": {    
    "@types/node": "^22.8.7",
    "axios": "^0.21.1",
    "form-data": "^4.0.0",
    "jose": "^4.11.4"
  }
}
